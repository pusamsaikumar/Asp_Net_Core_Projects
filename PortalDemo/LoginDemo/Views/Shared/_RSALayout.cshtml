    $(function () {
        function striptrailingslash(str) {
            if (str.substr(-1) == '/') {
                return str.substr(0, str.length - 1);
            }
            return str;
        }
        var has_sub = $('.has_sub');
        function openmenu(cls) {
         
            has_sub.children(cls).slidedown();
            has_sub.addclass('opened');
        }
        function closemenu(cls) {
           
            has_sub.children(cls).slideup();
            has_sub.removeclass('opened');
        }
        var url = "";
        url = window.location.pathname;

        var activepage = striptrailingslash(url);
        $('.sidebar-menu-ul li a').each(function () {
            var currentpage = striptrailingslash($(this).attr('href'));
            var new_url = "";
            new_url = currentpage;

            var n = activepage.split("/");
            if (activepage == new_url) {
                if ($(this).closest('ul').hasclass('sidebar-menu-ul-child-product')) {
                    $(this).parents('li').addclass('active');

                    if ($('.sidebar-menu-ul-child-product').hasclass('opened')) {
                        closemenu('.sidebar-menu-ul-child-product')
                    } else {
                        openmenu('.sidebar-menu-ul-child-product')
                    }
