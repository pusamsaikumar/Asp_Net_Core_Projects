                else if ($(this).closest('ul').hasclass('sidebar-menu-ul-child-shopper')) {
                  
                    $(this).parents('li').addclass('active');

                    if ($('.sidebar-menu-ul-child-shopper').hasclass('opened')) {
                        closemenu('.sidebar-menu-ul-child-shopper')
